<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cxf-codegen>
  <cxf-codegen-plugin>
    <plugin>
      <groupId>org.apache.cxf</groupId>
      <artifactId>cxf-codegen-plugin</artifactId>
      <version>${cxf.version}</version>
      <executions>
      </executions>
    </plugin>
  </cxf-codegen-plugin>
  <cxf-codegen-execution>
    <execution>
      <id>generate-sources-cxf-server</id>
      <phase>generate-sources</phase>
      <goals>
          <goal>wsdl2java</goal>
      </goals>
      <configuration>
        <sourceRoot>${basedir}/target/generated-sources/cxf/server</sourceRoot>
        <defaultOptions>
            <autoNameResolution>true</autoNameResolution>
            <extendedSoapHeaders>true</extendedSoapHeaders>
        </defaultOptions>
        <wsdlOptions>
          <wsdlOption>
            <wsdl></wsdl>
            <extraargs>
              <extraarg>-impl</extraarg>
            </extraargs>
          </wsdlOption>
        </wsdlOptions>
      </configuration>
    </execution>
  </cxf-codegen-execution>
</cxf-codegen>
